<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <route url="/V1/mailplus/subscribers" method="GET">
        <service class="MailPlus\MailPlus\Api\SubscriberRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="MailPlus_MailPlus::subscriber"/>
        </resources>
    </route>
    <route url="/V1/mailplus/store/:storeId/products" method="GET">
        <service class="MailPlus\MailPlus\Api\WebsiteProductRepositoryInterface" method="getStoreProductList"/>
        <resources>
            <resource ref="Magento_Catalog::products"/>
        </resources>
    </route>

</routes>